   "source": [
    "# 이항 계수1\n",
    "import sys\n",
    "\n",
    "input = sys.stdin.readline\n",
    "n, k = map(int, input().split())\n",
    "\n",
    "u = 1\n",
    "\n",
    "for i in range(k):\n",
    "    u *= n - i\n",
    "\n",
    "d = 1\n",
    "for i in range(k):\n",
    "    d *= k - i\n",
    "\n",
    "print(u // d)\n"
   ]
  }
