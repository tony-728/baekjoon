    "# 제로\n",
    "import sys\n",
    "\n",
    "k = int(input())\n",
    "answer = []\n",
    "for _ in range(k):\n",
    "    n = int(sys.stdin.readline())\n",
    "    if n == 0:\n",
    "        answer.pop()\n",
    "    else:\n",
    "        answer.append(n)\n",
    "\n",
    "print(sum(answer))"
   ]
  }
